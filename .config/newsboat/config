# browser "open -g -a 'Safari' %u"
# browser "open -a Safari -n"
browser "open -a Safari -g"
unbind-key j 
unbind-key J
unbind-key k
unbind-key K

bind-key j down
bind-key J next-feed
bind-key k up
bind-key K prev-feed
